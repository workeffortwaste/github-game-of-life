{
  "name": "GitHub Game of Life",
  "description": "Replace the GitHub contributions graph with Conway's Game of Life.",
  "version": "1.0.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "exclude_globs": ["https://github.com/*/*"],
      "js": ["github-game-of-life.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
